    "post_content": "<!-- wp:group {\"align\":\"wide\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"120px\",\"right\":\"0px\",\"bottom\":\"120px\",\"left\":\"0px\"},\"blockGap\":\"30px\"}},\"layout\":{\"inherit\":false}} -->\n<div class=\"wp-block-group alignwide\" style=\"padding-top:120px;padding-right:0px;padding-bottom:120px;padding-left:0px\"><!-- wp:columns {\"style\":{\"spacing\":{\"blockGap\":\"0px\"}}} -->\n<div class=\"wp-block-columns\"><!-- wp:column {\"width\":\"85%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:85%\"><!-- wp:heading {\"level\":1,\"fontSize\":\"heading-cta\"} -->\n<h1 class=\"has-heading-cta-font-size\">WordPress, publish your <em><mark class=\"has-inline-color has-deep-blueberry-color\">passion<\/mark><\/em><\/h1>\n<!-- \/wp:heading --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column {\"width\":\"15%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:15%\"><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns -->\n\n<!-- wp:paragraph {\"align\":\"left\"} -->\n<p class=\"has-text-align-left\">Create a place for your business, your interests, or anything else \u2013 with the open source platform that powers the web.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:buttons -->\n<div class=\"wp-block-buttons\"><!-- wp:button -->\n<div class=\"wp-block-button\"><a class=\"wp-block-button__link wp-element-button\" href=\"\/download\">Get WordPress<\/a><\/div>\n<!-- \/wp:button --><\/div>\n<!-- \/wp:buttons --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:wporg\/language-suggest {\"align\":\"full\"} -->\n<div class=\"wp-block-wporg-language-suggest alignfull\"><\/div>\n<!-- \/wp:wporg\/language-suggest -->\n\n<!-- wp:group {\"align\":\"full\",\"style\":{\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|blueberry-2\"}}}},\"backgroundColor\":\"charcoal-1\",\"textColor\":\"white\",\"layout\":{\"inherit\":true}} -->\n<div class=\"wp-block-group alignfull has-white-color has-charcoal-1-background-color has-text-color has-background has-link-color\" id=\"editor\"><!-- wp:cover {\"url\":\"https:\/\/wordpress.org\/main-test\/files\/2022\/08\/Background.webp\",\"id\":9185,\"dimRatio\":0,\"minHeight\":670,\"minHeightUnit\":\"px\",\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"80px\",\"right\":\"0px\",\"bottom\":\"80px\",\"left\":\"0px\"}}}} -->\n<div class=\"wp-block-cover alignfull\" style=\"padding-top:80px;padding-right:0px;padding-bottom:80px;padding-left:0px;min-height:670px\"><span aria-hidden=\"true\" class=\"wp-block-cover__background has-background-dim-0 has-background-dim\"><\/span><img class=\"wp-block-cover__image-background wp-image-9185\" alt=\"\" src=\"https:\/\/wordpress.org\/main-test\/files\/2022\/08\/Background.webp\" data-object-fit=\"cover\" \/><div class=\"wp-block-cover__inner-container\"><!-- wp:group {\"layout\":{\"inherit\":false,\"contentSize\":\"520px\"}} -->\n<div class=\"wp-block-group\"><!-- wp:heading {\"textAlign\":\"right\",\"textColor\":\"pomegrade-3\",\"fontSize\":\"heading-cta\"} -->\n<h2 class=\"has-text-align-right has-pomegrade-3-color has-text-color has-heading-cta-font-size\">Dream it, build it<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"left\",\"fontSize\":\"large\"} -->\n<p class=\"has-text-align-left has-large-font-size\">It\u2019s not magic, it\u2019s the WordPress Editor. Create custom sites with intuitive editing, flexible design tools, and powerful features to manage it all.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p><a rel=\"noreferrer noopener\" href=\"\/gutenberg\" target=\"_blank\">Explore the editor \u2197<\/a><\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group --><\/div><\/div>\n<!-- \/wp:cover --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"80px\",\"bottom\":\"80px\"}}},\"backgroundColor\":\"blueberry-4\",\"layout\":{\"inherit\":true}} -->\n<div class=\"wp-block-group alignfull has-blueberry-4-background-color has-background\" id=\"benefits\" style=\"padding-top:80px;padding-bottom:80px\"><!-- wp:columns {\"align\":\"wide\",\"style\":{\"spacing\":{\"blockGap\":\"100px\"}}} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"style\":{\"typography\":{\"fontSize\":\"50px\"},\"spacing\":{\"margin\":{\"bottom\":\"40px\",\"right\":\"100px\"}}}} -->\n<h2 style=\"margin-right:100px;margin-bottom:40px;font-size:50px\">Powerful and empowering<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:image {\"id\":8923,\"sizeSlug\":\"full\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-full\"><img src=\"https:\/\/wordpress.org\/main-test\/files\/2022\/07\/theme-styles.png\" alt=\"\" class=\"wp-image-8923\" \/><\/figure>\n<!-- \/wp:image --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column {\"style\":{\"spacing\":{\"blockGap\":\"5px\"}}} -->\n<div class=\"wp-block-column\"><!-- wp:paragraph {\"fontSize\":\"large\"} -->\n<p class=\"has-large-font-size\"><strong>Style it your way<\/strong><\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>Design anything you imagine. Start with a blank canvas or choose from a wide variety of <a href=\"https:\/\/wordpress.org\/themes\/\">themes<\/a> and <a href=\"https:\/\/wordpress.org\/patterns\/\">patterns<\/a>. Customize every detail, from color and fonts to layouts and functionality.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:spacer {\"height\":\"20px\"} -->\n<div style=\"height:20px\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer -->\n\n<!-- wp:paragraph {\"fontSize\":\"large\"} -->\n<p class=\"has-large-font-size\"><strong>Plug in and extend<\/strong><\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>Make WordPress do whatever you need it to. Add a store, mailing list, portfolio, social feed, analytics; you\u2019re in control with over 55,000 plugins.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:spacer {\"height\":\"20px\"} -->\n<div style=\"height:20px\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer -->\n\n<!-- wp:paragraph {\"fontSize\":\"large\"} -->\n<p class=\"has-large-font-size\"><strong>Own what you make&nbsp;<\/strong><\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>Your content, your design, and your data always belong to you. With WordPress you\u2019re free to tell your story, grow your brand, or simply be yourself.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:spacer {\"height\":\"20px\"} -->\n<div style=\"height:20px\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer -->\n\n<!-- wp:paragraph {\"fontSize\":\"large\"} -->\n<p class=\"has-large-font-size\"><strong>Create with confidence<\/strong><\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>Built by an open source community with decades of experience, its passionate contributors are committed to keeping WordPress as stable and secure as possible.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"80px\"}},\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|blueberry-2\"}}}},\"backgroundColor\":\"charcoal-1\",\"textColor\":\"white\",\"layout\":{\"inherit\":true}} -->\n<div class=\"wp-block-group alignfull has-white-color has-charcoal-1-background-color has-text-color has-background has-link-color\" id=\"showcase\" style=\"padding-top:80px\"><!-- wp:group {\"align\":\"wide\",\"layout\":{\"inherit\":false}} -->\n<div class=\"wp-block-group alignwide\"><!-- wp:heading {\"textAlign\":\"left\",\"style\":{\"typography\":{\"fontSize\":\"50px\"}}} -->\n<h2 class=\"has-text-align-left\" style=\"font-size:50px\">One platform, millions of possibilities<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"left\"} -->\n<p class=\"has-text-align-left\">Global creative agencies, local businesses, and even your<br>neighbor\u2019s personal blog are already using WordPress.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"align\":\"wide\",\"style\":{\"spacing\":{\"margin\":{\"top\":\"30px\",\"bottom\":\"30px\"}}},\"layout\":{\"type\":\"flex\",\"flexWrap\":\"nowrap\",\"justifyContent\":\"space-between\"}} -->\n<div class=\"wp-block-group alignwide\" style=\"margin-top:30px;margin-bottom:30px\"><!-- wp:image {\"id\":9099,\"width\":105,\"height\":33,\"sizeSlug\":\"full\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-full is-resized\"><img src=\"https:\/\/wordpress.org\/main-test\/files\/2022\/08\/Spotify.png\" alt=\"\" class=\"wp-image-9099\" width=\"105\" height=\"33\" \/><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:image {\"id\":9100,\"width\":79,\"height\":33,\"sizeSlug\":\"full\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-full is-resized\"><img src=\"https:\/\/wordpress.org\/main-test\/files\/2022\/08\/Time-Magazine.png\" alt=\"\" class=\"wp-image-9100\" width=\"79\" height=\"33\" \/><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:image {\"id\":9101,\"width\":118,\"height\":33,\"sizeSlug\":\"full\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-full is-resized\"><img src=\"https:\/\/wordpress.org\/main-test\/files\/2022\/08\/Vogue.png\" alt=\"\" class=\"wp-image-9101\" width=\"118\" height=\"33\" \/><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:image {\"id\":9098,\"width\":210,\"height\":33,\"sizeSlug\":\"full\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-full is-resized\"><img src=\"https:\/\/wordpress.org\/main-test\/files\/2022\/08\/Sony-Music.png\" alt=\"\" class=\"wp-image-9098\" width=\"210\" height=\"33\" \/><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:image {\"id\":9102,\"width\":97,\"height\":33,\"sizeSlug\":\"full\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-full is-resized\"><img src=\"https:\/\/wordpress.org\/main-test\/files\/2022\/08\/Wired.png\" alt=\"\" class=\"wp-image-9102\" width=\"97\" height=\"33\" \/><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:image {\"id\":9097,\"width\":76,\"height\":33,\"sizeSlug\":\"full\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-full is-resized\"><img src=\"https:\/\/wordpress.org\/main-test\/files\/2022\/08\/Disney.png\" alt=\"\" class=\"wp-image-9097\" width=\"76\" height=\"33\" \/><\/figure>\n<!-- \/wp:image --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:buttons {\"align\":\"wide\"} -->\n<div class=\"wp-block-buttons alignwide\"><!-- wp:button -->\n<div class=\"wp-block-button\"><a class=\"wp-block-button__link wp-element-button\" href=\"\/showcase\">Find inspiration<\/a><\/div>\n<!-- \/wp:button --><\/div>\n<!-- \/wp:buttons -->\n\n<!-- wp:spacer {\"height\":\"40px\"} -->\n<div style=\"height:40px\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer -->\n\n<!-- wp:image {\"align\":\"full\",\"id\":9190,\"sizeSlug\":\"full\",\"linkDestination\":\"none\",\"style\":{\"color\":[]}} -->\n<figure class=\"wp-block-image alignfull size-full\"><img src=\"https:\/\/wordpress.org\/main-test\/files\/2022\/08\/Showcase_2560.webp\" alt=\"\" class=\"wp-image-9190\" \/><\/figure>\n<!-- \/wp:image --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"160px\",\"bottom\":\"160px\"}},\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|white\"}}}},\"backgroundColor\":\"blueberry-1\",\"textColor\":\"white\",\"layout\":{\"inherit\":true}} -->\n<div class=\"wp-block-group alignfull has-white-color has-blueberry-1-background-color has-text-color has-background has-link-color\" id=\"learning\" style=\"padding-top:160px;padding-bottom:160px\"><!-- wp:columns {\"align\":\"wide\",\"style\":{\"spacing\":{\"blockGap\":\"100px\"}}} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"style\":{\"typography\":{\"fontSize\":\"50px\"}}} -->\n<h2 style=\"font-size:50px\">Build for yourself,<br>not by yourself<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph -->\n<p>Whether you\u2019re an entrepreneur, professional developer, or first-time blogger, there\u2019s a library of resources and learning tools ready for you. Plus, you have the whole WordPress community in your corner.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:list -->\n<ul><li><a href=\"https:\/\/learn.wordpress.org\/\">Learn with WordPress<\/a><\/li><li><a href=\"https:\/\/wordpress.org\/support\/\">Find support<\/a><\/li><li><a href=\"https:\/\/developer.wordpress.org\/\">Dig into the code<\/a><\/li><li><a href=\"https:\/\/www.meetup.com\/pro\/wordpress\/\">Meet local WordPress users<\/a><\/li><\/ul>\n<!-- \/wp:list -->\n\n<!-- wp:paragraph {\"textColor\":\"light-salmon\"} -->\n<p class=\"has-light-salmon-color has-text-color\">[Needs the link list style]<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"bottom\":\"100px\"}},\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|blueberry-1\"}}},\"color\":{\"background\":\"#f6f6f6\"}},\"textColor\":\"blueberry-1\",\"layout\":{\"inherit\":true}} -->\n<div class=\"wp-block-group alignfull has-blueberry-1-color has-text-color has-background has-link-color\" id=\"community\" style=\"background-color:#f6f6f6;padding-bottom:100px\"><!-- wp:cover {\"url\":\"https:\/\/wordpress.org\/main-test\/files\/2022\/08\/Community-Photo_2560.webp\",\"id\":9193,\"dimRatio\":0,\"focalPoint\":{\"x\":\"0.50\",\"y\":\"1.00\"},\"minHeight\":500,\"minHeightUnit\":\"px\",\"isDark\":false,\"align\":\"full\",\"style\":{\"color\":[],\"spacing\":{\"padding\":{\"top\":\"0px\",\"right\":\"0px\",\"bottom\":\"0px\",\"left\":\"0px\"},\"margin\":{\"bottom\":\"60px\"}}}} -->\n<div class=\"wp-block-cover alignfull is-light\" style=\"margin-bottom:60px;padding-top:0px;padding-right:0px;padding-bottom:0px;padding-left:0px;min-height:500px\"><span aria-hidden=\"true\" class=\"wp-block-cover__background has-background-dim-0 has-background-dim\"><\/span><img class=\"wp-block-cover__image-background wp-image-9193\" alt=\"\" src=\"https:\/\/wordpress.org\/main-test\/files\/2022\/08\/Community-Photo_2560.webp\" style=\"object-position:50% 100%\" data-object-fit=\"cover\" data-object-position=\"50% 100%\" \/><div class=\"wp-block-cover__inner-container\"><!-- wp:paragraph {\"align\":\"center\",\"fontSize\":\"large\"} -->\n<p class=\"has-text-align-center has-large-font-size\"><\/p>\n<!-- \/wp:paragraph --><\/div><\/div>\n<!-- \/wp:cover -->\n\n<!-- wp:heading {\"textAlign\":\"left\",\"align\":\"wide\",\"fontSize\":\"heading-cta\"} -->\n<h2 class=\"alignwide has-text-align-left has-heading-cta-font-size\"><em>Community<\/em> at its core<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:columns {\"align\":\"wide\",\"style\":{\"spacing\":{\"blockGap\":\"0px\"}}} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column {\"width\":\"33%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:33%\"><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column {\"width\":\"50%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:50%\"><!-- wp:paragraph -->\n<p>Behind the technology is a diverse collective of people, collaborating from around the world. We\u2019re united by the spirit of open source, and the freedom to build, transform, and share without barriers.&nbsp;<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>From writing code and testing, to community outreach and translation, there are so many ways to contribute and everyone is welcome. Let\u2019s shape the future of the web together.&nbsp;<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:buttons {\"layout\":{\"type\":\"flex\",\"justifyContent\":\"left\"}} -->\n<div class=\"wp-block-buttons\"><!-- wp:button {\"className\":\"is-style-outline\"} -->\n<div class=\"wp-block-button is-style-outline\"><a class=\"wp-block-button__link wp-element-button\" href=\"https:\/\/make.wordpress.org\/\">Get involved<\/a><\/div>\n<!-- \/wp:button --><\/div>\n<!-- \/wp:buttons --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column {\"width\":\"16%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:16%\"><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:columns {\"align\":\"wide\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"80px\",\"bottom\":\"80px\"},\"blockGap\":\"0px\"}},\"className\":\"is-style-default\"} -->\n<div class=\"wp-block-columns alignwide is-style-default\" id=\"news\" style=\"padding-top:80px;padding-bottom:80px\"><!-- wp:column {\"verticalAlignment\":\"top\",\"width\":\"33%\",\"className\":\"is-left-column\",\"layout\":{\"inherit\":false}} -->\n<div class=\"wp-block-column is-vertically-aligned-top is-left-column\" style=\"flex-basis:33%\"><!-- wp:group {\"style\":{\"spacing\":{\"padding\":{\"right\":\"80px\"}}},\"layout\":{\"type\":\"default\"}} -->\n<div class=\"wp-block-group\" style=\"padding-right:80px\"><!-- wp:heading {\"style\":{\"typography\":{\"fontSize\":\"50px\"}}} -->\n<h2 style=\"font-size:50px\">See what's new in WordPress<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:social-links -->\n<ul class=\"wp-block-social-links\"><!-- wp:social-link {\"url\":\"https:\/\/twitter.com\/WordPress\/\",\"service\":\"twitter\"} \/-->\n\n<!-- wp:social-link {\"url\":\"https:\/\/www.facebook.com\/WordPress\/\",\"service\":\"facebook\"} \/--><\/ul>\n<!-- \/wp:social-links --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column {\"width\":\"66%\",\"layout\":{\"inherit\":false}} -->\n<div class=\"wp-block-column\" style=\"flex-basis:66%\"><!-- wp:wporg\/multisite-latest-posts -->\n<div class=\"wp-block-wporg-multisite-latest-posts\" data-endpoint=\"https:\/\/wordpress.org\/news\/wp-json\/wp\/v2\" data-per-page=\"3\"><\/div>\n<!-- \/wp:wporg\/multisite-latest-posts -->\n\n<!-- wp:spacer {\"height\":\"20px\"} -->\n<div style=\"height:20px\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer -->\n\n<!-- wp:paragraph -->\n<p><a href=\"https:\/\/wordpress.org\/news\/\">View all<\/a><\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns -->\n\n<!-- wp:group {\"align\":\"full\",\"style\":{\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|white\"}}},\"spacing\":{\"padding\":{\"top\":\"80px\",\"bottom\":\"80px\"}}},\"backgroundColor\":\"blueberry-1\",\"textColor\":\"white\",\"layout\":{\"inherit\":true}} -->\n<div class=\"wp-block-group alignfull has-white-color has-blueberry-1-background-color has-text-color has-background has-link-color\" id=\"get-started\" style=\"padding-top:80px;padding-bottom:80px\"><!-- wp:group {\"align\":\"wide\",\"style\":{\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|white\"}}}},\"className\":\"wporg-pattern__heading-with-arrow\"} -->\n<div class=\"wp-block-group alignwide wporg-pattern__heading-with-arrow has-link-color\"><!-- wp:heading {\"style\":{\"typography\":{\"fontStyle\":\"normal\",\"fontWeight\":\"300\",\"lineHeight\":\"1.1\",\"letterSpacing\":\"-0.02em\"}},\"className\":\"has-inter-font-family\",\"fontSize\":\"heading-cta\"} -->\n<h2 class=\"has-inter-font-family has-heading-cta-font-size\" style=\"font-style:normal;font-weight:300;letter-spacing:-0.02em;line-height:1.1\"><a href=\"https:\/\/wordpress.org\/main-test\/download\/\" data-type=\"page\" data-id=\"371\">Get Started<\/a><\/h2>\n<!-- \/wp:heading --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:columns {\"align\":\"wide\"} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column {\"width\":\"50%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:50%\"><!-- wp:paragraph -->\n<p>Find everything you need to get started, download the platform, find hosting, and more \u2013 whether it\u2019s your first site or your ninety-first site. Get WordPress now.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column {\"width\":\"50%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:50%\"><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns --><\/div>\n<!-- \/wp:group -->",
