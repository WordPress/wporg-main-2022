    "post_content": "<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"6rem\",\"right\":\"4rem\",\"bottom\":\"6rem\",\"left\":\"4rem\"}}},\"layout\":{\"inherit\":true}} -->\n<div class=\"wp-block-group alignfull\" style=\"padding-top:6rem;padding-right:4rem;padding-bottom:6rem;padding-left:4rem\"><!-- wp:heading {\"textAlign\":\"center\",\"level\":1,\"style\":{\"typography\":{\"lineHeight\":1.1,\"fontSize\":\"70px\"}}} -->\n<h1 class=\"has-text-align-center\" style=\"font-size:70px;line-height:1.1\">Get WordPress<\/h1>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"center\",\"className\":\"is-style-serif\",\"fontSize\":\"heading-5\"} -->\n<p class=\"has-text-align-center is-style-serif has-heading-5-font-size\">Everything you need to set up your site just the way you want it.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"align\":\"full\",\"style\":{\"border\":{\"top\":{\"color\":\"var:preset|color|light-grey-1\",\"width\":\"1px\"}}},\"layout\":{\"inherit\":true}} -->\n<div class=\"wp-block-group alignfull\" style=\"border-top-color:var(--wp--preset--color--light-grey-1);border-top-width:1px\"><!-- wp:columns {\"align\":\"wide\",\"style\":{\"spacing\":{\"blockGap\":\"0px\"}}} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column {\"width\":\"50%\",\"style\":{\"border\":{\"right\":{\"color\":\"var:preset|color|light-grey-1\",\"width\":\"1px\"}}}} -->\n<div class=\"wp-block-column\" style=\"border-right-color:var(--wp--preset--color--light-grey-1);border-right-width:1px;flex-basis:50%\"><!-- wp:group {\"align\":\"wide\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"80px\",\"right\":\"60px\",\"bottom\":\"80px\",\"left\":\"0px\"}}}} -->\n<div class=\"wp-block-group alignwide\" id=\"download-install\" style=\"padding-top:80px;padding-right:60px;padding-bottom:80px;padding-left:0px\"><!-- wp:heading {\"textAlign\":\"left\",\"fontSize\":\"huge\"} -->\n<h2 class=\"has-text-align-left has-huge-font-size\">Download and install yourself<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"left\"} -->\n<p class=\"has-text-align-left\">For anyone comfortable getting their own hosting and domain.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:buttons {\"layout\":{\"type\":\"flex\",\"justifyContent\":\"left\"},\"style\":{\"spacing\":{\"blockGap\":\"10px\"}}} -->\n<div class=\"wp-block-buttons\"><!-- wp:button -->\n<div class=\"wp-block-button\" id=\"wporg__download-button\"><a class=\"wp-block-button__link wp-element-button\" href=\"[download_link]\">Download WordPress [latest_version]<\/a><\/div>\n<!-- \/wp:button -->\n\n<!-- wp:button {\"textColor\":\"blue-1\",\"className\":\"is-style-outline\"} -->\n<div class=\"wp-block-button is-style-outline\"><a class=\"wp-block-button__link has-blue-1-color has-text-color wp-element-button\" href=\"https:\/\/wordpress.org\/support\/article\/how-to-install-wordpress\/\">Installation guide<\/a><\/div>\n<!-- \/wp:button --><\/div>\n<!-- \/wp:buttons -->\n\n<!-- wp:paragraph {\"textColor\":\"charcoal-4\",\"fontSize\":\"small\"} -->\n<p class=\"has-charcoal-4-color has-text-color has-small-font-size\">Recommend PHP [recommended_php] or greater and MySQL [recommended_mysql] or MariaDB version [recommended_mariadb] or greater.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column {\"width\":\"50%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:50%\"><!-- wp:group {\"align\":\"wide\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"80px\",\"right\":\"0px\",\"bottom\":\"80px\",\"left\":\"60px\"}}}} -->\n<div class=\"wp-block-group alignwide\" id=\"hosting\" style=\"padding-top:80px;padding-right:0px;padding-bottom:80px;padding-left:60px\"><!-- wp:heading {\"textAlign\":\"left\",\"fontSize\":\"huge\"} -->\n<h2 class=\"has-text-align-left has-huge-font-size\">Set up with a hosting provider<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"left\"} -->\n<p class=\"has-text-align-left\">For anyone looking for the simplest way to start.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:buttons -->\n<div class=\"wp-block-buttons\"><!-- wp:button -->\n<div class=\"wp-block-button\"><a class=\"wp-block-button__link wp-element-button\" href=\"https:\/\/wordpress.org\/main-test\/hosting\/\">See all recommended hosts<\/a><\/div>\n<!-- \/wp:button --><\/div>\n<!-- \/wp:buttons --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"align\":\"full\",\"backgroundColor\":\"blueberry-4\",\"layout\":{\"inherit\":true}} -->\n<div class=\"wp-block-group alignfull has-blueberry-4-background-color has-background\" id=\"features\"><!-- wp:columns {\"verticalAlignment\":\"center\",\"align\":\"wide\"} -->\n<div class=\"wp-block-columns alignwide are-vertically-aligned-center\"><!-- wp:column {\"verticalAlignment\":\"center\"} -->\n<div class=\"wp-block-column is-vertically-aligned-center\"><!-- wp:list {\"className\":\"is-style-features\"} -->\n<ul class=\"is-style-features\"><li>Simple<\/li><li>Intuitive<\/li><li>Extendable<\/li><li>Free<\/li><li>Open<\/li><\/ul>\n<!-- \/wp:list --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column {\"verticalAlignment\":\"center\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"60px\",\"right\":\"60px\",\"bottom\":\"60px\",\"left\":\"60px\"}}},\"layout\":{\"inherit\":false}} -->\n<div class=\"wp-block-column is-vertically-aligned-center\" style=\"padding-top:60px;padding-right:60px;padding-bottom:60px;padding-left:60px\"><!-- wp:heading {\"fontSize\":\"huge\"} -->\n<h2 class=\"has-huge-font-size\">Powerful right out of the box<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph -->\n<p>WordPress combines simplicity for users and publishers with under-the-hood complexity for developers. Discover the features that come standard with WordPress, knowing that you can extend what the platform can do with the thousands of plugins available.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p><a href=\"https:\/\/wordpress.org\/about\/features\/\">See all WordPress features \u2197<\/a><\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"6rem\",\"right\":\"4rem\",\"bottom\":\"6rem\",\"left\":\"4rem\"}},\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|white\"}}}},\"backgroundColor\":\"blueberry-1\",\"textColor\":\"white\",\"layout\":{\"inherit\":true}} -->\n<div class=\"wp-block-group alignfull has-white-color has-blueberry-1-background-color has-text-color has-background has-link-color\" id=\"resources\" style=\"padding-top:6rem;padding-right:4rem;padding-bottom:6rem;padding-left:4rem\"><!-- wp:columns {\"align\":\"wide\"} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"style\":{\"typography\":{\"fontSize\":\"50px\",\"lineHeight\":\"1.1\"}}} -->\n<h2 style=\"font-size:50px;line-height:1.1\">You've got WordPress, what's next?<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph -->\n<p>If you need help getting started, these resources can help you find your way.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:list -->\n<ul><li><a href=\"https:\/\/learn.wordpress.org\/course\/getting-started-with-wordpress-get-setup\/\">WordPress Courses \u2197<\/a><\/li><li><a href=\"https:\/\/developer.wordpress.org\/\">Developer Resources \u2197<\/a><\/li><li><a href=\"https:\/\/wordpress.org\/support\/\">WordPress Support \u2197<\/a><\/li><li><a href=\"https:\/\/wordpress.org\/support\/forum\/installation\/\">User Forums \u2197<\/a><\/li><\/ul>\n<!-- \/wp:list -->\n\n<!-- wp:paragraph {\"textColor\":\"light-salmon\"} -->\n<p class=\"has-light-salmon-color has-text-color\">[Link list style]<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"align\":\"wide\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"4rem\",\"right\":\"4rem\",\"bottom\":\"4rem\",\"left\":\"4rem\"}}}} -->\n<div class=\"wp-block-group alignwide\" id=\"mobile\" style=\"padding-top:4rem;padding-right:4rem;padding-bottom:4rem;padding-left:4rem\"><!-- wp:heading {\"textAlign\":\"center\",\"style\":{\"typography\":{\"fontSize\":\"50px\",\"lineHeight\":\"1.1\"}}} -->\n<h2 class=\"has-text-align-center\" style=\"font-size:50px;line-height:1.1\">Inspiration strikes anywhere, anytime<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"center\"} -->\n<p class=\"has-text-align-center\">Create or update content on the go with our mobile apps.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph {\"align\":\"center\"} -->\n<p class=\"has-text-align-center\"><a href=\"https:\/\/wordpress.org\/mobile\/\" data-type=\"URL\" data-id=\"https:\/\/wordpress.org\/mobile\/\">Learn more about our mobile apps<\/a><\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:group {\"layout\":{\"type\":\"flex\",\"flexWrap\":\"nowrap\",\"justifyContent\":\"center\"}} -->\n<div class=\"wp-block-group\"><!-- wp:image {\"width\":150,\"height\":45,\"linkDestination\":\"custom\"} -->\n<figure class=\"wp-block-image is-resized\"><a href=\"https:\/\/itunes.apple.com\/app\/apple-store\/id335703880?pt=299112&amp;ct=wordpress.org&amp;mt=8\"><img src=\"https:\/\/wordpress.org\/wp-content\/themes\/pub\/wporg-main\/images\/badge-apple.png\" alt=\"Available in the Apple App Store\" width=\"150\" height=\"45\" \/><\/a><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:image {\"width\":150,\"height\":45,\"linkDestination\":\"custom\"} -->\n<figure class=\"wp-block-image is-resized\"><a href=\"http:\/\/play.google.com\/store\/apps\/details?id=org.wordpress.android\"><img src=\"https:\/\/wordpress.org\/wp-content\/themes\/pub\/wporg-main\/images\/badge-google-play.png\" alt=\"Available in the Google Play Store\" width=\"150\" height=\"45\" \/><\/a><\/figure>\n<!-- \/wp:image --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:spacer -->\n<div style=\"height:100px\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer -->",
