{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wporg/remembers-list",
  "version": "0.1.0",
  "title": "Remembers Contributor List",
  "category": "design",
  "icon": "list-view",
  "description": "Displays a list of memorialized contributors.",
  "textdomain": "wporg",
  "attributes": {
    "columns": {
      "type": "number",
      "default": 3
    }
  },
  "supports": {
    "align": true,
    "color": {
      "background": true,
      "text": true
    },
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ],
      "padding": true,
      "blockGap": false
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}